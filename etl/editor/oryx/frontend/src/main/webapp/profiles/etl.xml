		<plugin engaged="true" name="ORYX.Plugins.LabelLayout" source="labelLayout.js"/>


		<plugin engaged="true" name="ORYX.Plugins.WaveThis" source="waveThis.js">
			<requires namespace="http://b3mn.org/stencilset/bpmn2.0#"/>
			<requires namespace="http://b3mn.org/stencilset/UML2.2Class#"/>
			<requires namespace="http://www.fmc-modeling.org/stencilsets/fmcblockdiagram#"/>
			<requires namespace="http://b3mn.org/stencilset/epc#"/>
			<requires namespace="http://b3mn.org/stencilset/petrinet#"/>
		</plugin>

		<plugin engaged="true" name="ORYX.Plugins.Timeline" source="timeline.js">
			<requires namespace="http://matthias-kunze.info/oryx/stencilsets/timeline#"/>
		</plugin>


        <!-- ETL -->
        <plugin source="etl/metadata/metadataeditorrepository.js" name="ORYX.Plugins.ETL.Metadata.EditorRepository"/>
<!--        <plugin source="etl/repository/explorer/etlreponavigation.js" name="ORYX.Plugins.ETLRepoNavigation"/>
        <plugin source="etl/repository/explorer/docreponavigation.js" name="ORYX.Plugins.DOCRepoNavigation"/>
        <plugin source="etl/metadata/metadataeditorrepository.js" name="ORYX.Plugins.ETL.Metadata.EditorRepository"/>
        <plugin source="etl/metadata/stepmetadatapropertyeditor.js" name="ORYX.Plugins.ETL.Metadata.StepMetadataPropertyEditor"/>-->
	</plugins>

	
	<properties>
        <property group="New" index="1"/>
		<property group="File" index="2"/>
		<property group="Edit" index="3"/>
		<property group="Undo" index="4"/>
        <!-- ZZZZ assures that it is always at the end Feedback Plugin, ... -->
        <property group="Help" index="ZZZZZZ"/> 	
